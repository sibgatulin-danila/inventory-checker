        <section class="forms">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="section-heading">
                            <h2><%- equipmentType.name %></h2>
                        </div>
                        <form id="contact" action="<%- equipmentTypesUpdateUrl %>" method="post">
                            <div class="row">
                                <div class="col-md-6" hidden>
                                    <fieldset hidden>
                                        <input
                                            name="_id"
                                            class="form-control"
                                            id="_id"
                                            hidden
                                            required=""
                                            value="<%- equipmentType._id %>"
                                        >
                                    </fieldset>
                                </div>
                                <div class="col-md-12">
                                    <fieldset>
                                        <input
                                            name="name"
                                            type="text"
                                            class="form-control"
                                            id="name"
                                            placeholder="Наименование..."
                                            required=""
                                            value="<%- equipmentType.name %>"
                                        >
                                    </fieldset>
                                </div>
                                <div class="col-md-12">
                                    <button type="submit" id="form-submit" class="button">Обновить информацию</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>

