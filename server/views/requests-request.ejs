<%- include('partials/head') %>

<div id="main">
    <div class="inner">

        <%- include('partials/header') %>

        <!-- Tables -->
        <section class="forms">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="section-heading">
                            <h2>Компьютер 1</h2>
                        </div>
                        <form id="contact" action="" method="post">
                            <div class="row">
                                <div class="col-md-6">
                                    <select name="category" id="category">
                                        <option value="categories" selected>Сотрудник</option>
                                        <option value="Featured">Иванов А.А.</option>
                                        <option value="Newest">Сибгатулин Д.С.</option>
                                        <option value="Low Price">Technical</option>
                                        <option value="High Price">Application</option>
                                    </select>
                                </div>
                                <div class="col-md-6">
                                    <fieldset>
                                        <input name="phone" type="text" class="form-control" id="phone" placeholder="Телефон" required="">
                                    </fieldset>
                                </div>
                                <div class="col-md-12">
                                    <fieldset>
                                        <input name="username" type="text" class="form-control" id="phone" placeholder="Username" required="">
                                    </fieldset>
                                </div>
                                <div class="col-12">
                                    <textarea name="demo-message" id="demo-message" placeholder="Примечания" rows="6"></textarea>
                                </div>
                                <div class="col-md-12">
                                    <button type="submit" id="form-submit" class="button">Обновить информацию</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>

    </div>
</div>

<%- include('partials/sidebar') %>

<%- include('partials/footer') %>
